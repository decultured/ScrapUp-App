<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" 
        xmlns:fb="http://ns.adobe.com/flashbuilder/2009" alpha.disabled="0.5">
	    <fx:Metadata>
	        <![CDATA[ 
	        [HostComponent("spark.components.SkinnableContainer")]
	        ]]>
	    </fx:Metadata> 
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	<fx:Script>
		<![CDATA[

		]]>
	</fx:Script>
	
	<!-- Radial Gradient Overlay -->
	<s:Rect height="100%" width="100%" blendMode="multiply">
		<s:fill>
			<s:RadialGradient interpolationMethod="linearRGB" scaleX="{contentGroup.width * 1.3}" scaleY="{contentGroup.height * 1.5}" x="{contentGroup.width/2}" y="{contentGroup.height/2}">
				<s:GradientEntry color="#FFFFFF" ratio="0" />
				<s:GradientEntry color="#333333" ratio="1" />
			</s:RadialGradient>
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" left="0" right="0" top="0" bottom="0" minWidth="0" minHeight="0">
		<s:layout>
			<s:BasicLayout/>
		</s:layout>
	</s:Group>
</s:Skin>